function plugin_updatepriceswc() {
	if (current_user_can('manage_option'){
		add_menu_page( 
			'Actualización de precios', 
			'Actualización de precios',
			'read', 
			'updatepriceswc', 
			'updatepriceswc_options',
			'dashicons-clipboard'
		);
	}
}
add_action('admin_menu', 'plugin_updatepriceswc');
